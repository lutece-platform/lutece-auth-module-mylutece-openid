<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    <name>mylutece-openid</name>
    <class>fr.paris.lutece.plugins.mylutece.modules.openid.service.OpenIDPlugin</class>
    <version>1.1.1</version>
    <description>module.mylutece.openid.module.description</description>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <provider>module.mylutece.openid.module.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/mylutece/mylutece.png</icon-url>
    <copyright>Copyright 2001-2009 Mairie de Paris</copyright>
	<core-version-dependency>
		<min-core-version>2.4.1</min-core-version>
		<max-core-version/>	
	</core-version-dependency>
	
	<!-- if the plugin must have a connection pool with parameter : 1 - yes, 0 - no -->	
    <db-pool-required>0</db-pool-required>
    
    <!-- Specific plugin CSS stylesheets -->
    <css-stylesheets>
        <css-stylesheet>mylutece/modules/openid/openid.css</css-stylesheet>
    </css-stylesheets>

    <!-- Applications -->
    <applications>
        <application>
            <application-id>openid</application-id>
            <application-class>fr.paris.lutece.plugins.mylutece.modules.openid.web.OpenIDApp</application-class>
        </application>
    </applications>
    
    
</plug-in>